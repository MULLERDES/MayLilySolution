<Window x:Class="MayLily.CommonViews.Seller.NomenclatureMain"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MayLily.CommonViews.ViewModels.Common"
        mc:Ignorable="d"
        Title="NomenclatureMain" Height="300" Width="300">
    <Window.DataContext>
        <local:NomenclatureModel x:Name="ddd"></local:NomenclatureModel>
    </Window.DataContext>
    <Grid>
        <ListView ItemsSource="{Binding Source=ddd}"> 
            
        </ListView>
    </Grid>
</Window>
